<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="574.0" prefWidth="893.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Templates.MaisonGui.AddMaisonController">
   <children>
      <VBox layoutX="40.0" layoutY="39.0" prefHeight="369.0" prefWidth="130.0" spacing="15.0">
         <children>
            <Label text="Nom" />
            <TextField fx:id="txt_nom" />
            <Label layoutX="10.0" layoutY="10.0" text="Adress" />
            <TextField fx:id="txt_adress" layoutX="10.0" layoutY="26.0" />
            <Label layoutX="10.0" layoutY="50.0" text="Description" />
            <TextField fx:id="txt_description" layoutX="10.0" layoutY="66.0" />
            <Label layoutX="10.0" layoutY="90.0" text="Prix" />
            <TextField fx:id="txt_prix" layoutX="10.0" layoutY="106.0" />
